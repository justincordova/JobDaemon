{
  "name": "job-daemon",
  "version": "1.0.0",
  "description": "Background service to scrape and notify about new SWE internships",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "node --no-deprecation --import ./loader.mjs src/index.ts",
    "build": "puppeteer browsers install chrome && tsc",
    "start": "node dist/index.js",
    "test:notify": "node --no-deprecation --import ./loader.mjs src/test/test-notification.ts",
    "test:email": "node --no-deprecation --import ./loader.mjs src/test/test-email.ts",
    "test:ping": "node --no-deprecation --import ./loader.mjs src/test/test-ping.ts",
    "test:download": "node --no-deprecation --import ./loader.mjs src/test/test-download.ts",
    "clear-db": "rm jobs.db"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "better-sqlite3": "^12.4.6",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "firecrawl": "^4.6.2",
    "node-cron": "^4.2.1",
    "puppeteer": "^24.31.0",
    "resend": "^6.5.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "winston": "^3.18.3"
  }
}
